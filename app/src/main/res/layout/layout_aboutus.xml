<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/viewAbout_line1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:padding="5dp"
        android:layout_marginTop="10dp"
        android:text="Line 1"
        android:textColor="#778899"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewAbout_line1" />

    <TextView
        android:id="@+id/viewAbout_line2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:padding="5dp"
        android:text="Line 2"
        android:textColor="#778899"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view1" />
    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewAbout_line2" />

    <TextView
        android:id="@+id/viewAbout_line3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:padding="5dp"
        android:text="Line 3"
        android:textColor="#778899"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewAbout_line3" />

    <TextView
        android:id="@+id/viewAbout_line4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:padding="5dp"
        android:text="Line 4"
        android:textColor="#778899"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view3" />


    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewAbout_line4" />

    <TextView
        android:id="@+id/viewAbout_line5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:padding="5dp"
        android:text="Line 5"
        android:textColor="#778899"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view4" />
    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewAbout_line5" />
</androidx.constraintlayout.widget.ConstraintLayout>