<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/inquirynm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:hint="Name"
        android:paddingLeft="10dp"
        android:textSize="18sp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/viewnm"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/viewnm"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintTop_toBottomOf="@id/inquirynm"/>

    <TextView
        android:id="@+id/inquiryemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:hint="Email"
        android:textSize="18sp"
        android:paddingLeft="10dp"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewnm" />
    <View
        android:id="@+id/viewemail"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintTop_toBottomOf="@id/inquiryemail"/>
    <TextView
        android:id="@+id/inquirycontact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:hint="Contact"
        android:textSize="18sp"
        android:paddingLeft="10dp"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewemail" />

    <View
        android:id="@+id/viewcontact"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintTop_toBottomOf="@id/inquirycontact"/>
    <TextView
        android:id="@+id/inquiryaskanything"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:hint="Ask Anything"
        android:textSize="18sp"
        android:paddingLeft="10dp"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewcontact" />
    <View
        android:id="@+id/viewask"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f00"
        app:layout_constraintTop_toBottomOf="@id/inquiryaskanything"/>

    <TextView
        android:id="@+id/inquiryfeedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/zilla_slab_semibold_italic"
        android:hint="Feedback"
        android:text=""
        android:textSize="18sp"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewask" />
    <View
        android:id="@+id/viewfeedback"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#090909"
        app:layout_constraintTop_toBottomOf="@id/inquiryfeedback"/>

</androidx.constraintlayout.widget.ConstraintLayout>